<!DOCTYPE html>
<html lang="en">
  <head>
    {{>head}}
  </head>
  <body>
    
	  {{>nav}}
    <!-- END nav -->
    
    <section class="hero-wrap hero-wrap-2" style="background-image: url('images/menu-background.jpg');" data-stellar-background-ratio="0.5">
      <div class="overlay"></div>
      <div class="container">
        <div class="row no-gutters slider-text align-items-center justify-content-center">
          <div class="col-md-9 ftco-animate text-center">
            <h1 class="mb-2 bread">Specialties</h1>
            <p class="breadcrumbs"><span class="mr-2"><a href="/">Home <i class="ion-ios-arrow-forward"></i></a></span> <span>Menu <i class="ion-ios-arrow-forward"></i></span></p>
          </div>
        </div>
      </div>
    </section>
	

		<section class="ftco-section">
    	<div class="container">
    		<div class="row justify-content-center mb-5 pb-2">
          <div class="col-md-7 text-center heading-section ftco-animate">
          	<span class="subheading">Specialties</span>
            <h2 class="mb-4">Our Menu</h2>
          </div>
        </div>
		<div class="row">
        	<div class="col-md-6 col-lg-4 menu-wrap">
        		<div class="heading-menu text-center ftco-animate">
        			<h3>Coffee</h3>
        		</div>
        		<div class="menus d-flex ftco-animate">
              <div class="menu-img img" style="background-image: url(images/coffee1.jpg);"></div>
              <div class="text">
              	<div class="d-flex">
	                <div class="one-half">
	                  <h3>Cappuccino</h3>
	                </div>
	                <div class="one-forth">
	                  <span class="price">₹100</span>
	                </div>
	              </div>
	              <p><span>Milk</span>, <span>Sugar</span>, <span>Ground cinnamon</span></p>
              </div>
            </div>
            <div class="menus d-flex ftco-animate">
              <div class="menu-img img" style="background-image: url(images/coffee2.jpg);"></div>
              <div class="text">
              	<div class="d-flex">
	                <div class="one-half">
	                  <h3>Black Coffee</h3>
	                </div>
	                <div class="one-forth">
	                  <span class="price">₹80</span>
	                </div>
	              </div>
	              <p><span>Water</span>, <span>Brown Sugar</span></p>
              </div>
            </div>
            <div class="menus d-flex ftco-animate">
              <div class="menu-img img" style="background-image: url(images/coffee3.jpg);"></div>
              <div class="text">
              	<div class="d-flex">
	                <div class="one-half">
	                  <h3>Espresso</h3>
	                </div>
	                <div class="one-forth">
	                  <span class="price">₹100</span>
	                </div>
	              </div>
	              <p><span>cold water</span>, <span>ground coffee (French or other dark roast)</span></p>
              </div>
			</div>
			
			</div>

        	<div class="col-md-6 col-lg-4 menu-wrap">
        		<div class="heading-menu text-center ftco-animate">
        			<h3>Tea</h3>
        		</div>
        		<div class="menus d-flex ftco-animate">
              <div class="menu-img img" style="background-image: url(images/tea1.jpg);"></div>
              <div class="text">
              	<div class="d-flex">
	                <div class="one-half">
	                  <h3>Iced Tea</h3>
	                </div>
	                <div class="one-forth">
	                  <span class="price">₹60</span>
	                </div>
	              </div>
	              <p><span>Black tea</span>, <span>sugar</span>, <span>lemon juice</span></p>
              </div>
            </div>
            <div class="menus d-flex ftco-animate">
              <div class="menu-img img" style="background-image: url(images/tea2.jpg);"></div>
              <div class="text">
              	<div class="d-flex">
	                <div class="one-half">
	                  <h3>Black Tea</h3>
	                </div>
	                <div class="one-forth">
	                  <span class="price">₹50</span>
	                </div>
	              </div>
	              <p><span>Water</span>, <span>Tea leaves</span>, <span>Sugar</span></p>
              </div>
            </div>
            <div class="menus d-flex ftco-animate">
              <div class="menu-img img" style="background-image: url(images/tea3.jpg);"></div>
              <div class="text">
              	<div class="d-flex">
	                <div class="one-half">
	                  <h3>Dargeeling Masala Tea</h3>
	                </div>
	                <div class="one-forth">
	                  <span class="price">₹80</span>
	                </div>
	              </div>
	              <p><span>pepper powder</span>, <span> cardamom pods</span>, <span> milk</span>, <span> grated ginger</span></p>
              </div>
            </div>
        	</div>

        	<div class="col-md-6 col-lg-4 menu-wrap">
        		<div class="heading-menu text-center ftco-animate">
        			<h3>Cupcake</h3>
        		</div>
        		<div class="menus d-flex ftco-animate">
              <div class="menu-img img" style="background-image: url(images/cupcake1.jpg);"></div>
              <div class="text">
              	<div class="d-flex">
	                <div class="one-half">
	                  <h3>Chocolate Frosting</h3>
	                </div>
	                <div class="one-forth">
	                  <span class="price">₹90</span>
	                </div>
	              </div>
	              <p><span>cocoa powder</span>, <span> butter</span>, <span> milk</span>, <span> vanilla extract</span></p>
              </div>
            </div>
            <div class="menus d-flex ftco-animate">
              <div class="menu-img img" style="background-image: url(images/cupcake2.jpg);"></div>
              <div class="text">
              	<div class="d-flex">
	                <div class="one-half">
	                  <h3>Lavender & Vanilla</h3>
	                </div>
	                <div class="one-forth">
	                  <span class="price">₹150</span>
	                </div>
	              </div>
	              <p><span>eggs</span>, <span>culinary lavender</span>, <span>Flour</span>, <span> caster sugar</span></p>
              </div>
            </div>
            <div class="menus d-flex ftco-animate">
              <div class="menu-img img" style="background-image: url(images/cupcake3.jpg);"></div>
              <div class="text">
              	<div class="d-flex">
	                <div class="one-half">
	                  <h3>Neapolitan</h3>
	                </div>
	                <div class="one-forth">
	                  <span class="price">₹120</span>
	                </div>
	              </div>
	              <p><span>Baking soda</span>, <span>Eggs</span>, <span>Vegetable oil</span>, <span> cream</span></p>
              </div>
            </div>
        	</div>
        
        </div>
    	</div>

        	<!--  -->
        	

    </section>


	{{>footer}}

	{{>scripts}}
    
  </body>
</html>